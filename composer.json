{
    "name": "hotelbeds/hotel-api-sdk-php",
    "description": "Hotelbeds Hotel-API SDK for PHP",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "laminas/laminas-config": ">=3.3.0",
        "laminas/laminas-dependency-plugin": "^2.1",
        "laminas/laminas-http": ">=2.11.2",
        "laminas/laminas-json": ">=3.1.2",
        "laminas/laminas-uri": ">=2.9.1",
        "nicmart/string-template": ">=0.1.2",
        "laminas/laminas-stdlib": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Tomeu Capo",
            "email": "t.capo@hotelbeds.com"
        },
        {
            "name": "Thorbj√∂rn 'Puggan' Sundragon",
            "email": "thorbjorn.sundragon@rolfsbuss.se"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "hotelbeds\\hotel_api_sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hotelbeds\\hotel_api_sdk\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        }
    }
}
